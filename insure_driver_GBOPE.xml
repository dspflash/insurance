<GroupBox cols="xs12 sm12 md12 lg12" Name="GBOPEinsure_driver" Size="100,100"><Ctrls>
<Button sqlpkinsert="driid" Text="create" ToolTip="ttp_createinsure_driver" Name="insertinsure_driver" Size="50,20">
